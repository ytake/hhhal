{
  "name": "ytake/hhypermedia",
  "description": "for HATEOAS REST web services",
  "keywords": [
    "hal",
    "Hypertext Application Language",
    "rest",
    "serialize"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "hhvm": "^4.25",
    "hhvm/hsl": "^4.0",
    "hhvm/hsl-experimental": "^4.25",
    "hhvm/hhvm-autoload": "^2.0.10"
  },
  "require-dev": {
    "hhvm/hacktest": "^2.0.0",
    "facebook/fbexpect": "^2.7.3",
    "hhvm/hhast": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\Hhypermedia\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "tests": [
      "hhvm ./vendor/bin/hacktest.hack tests/"
    ],
    "register": [
      "hhvm ./vendor/bin/hh-autoload.hack"
    ]
  }
}
